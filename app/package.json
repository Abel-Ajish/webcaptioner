{
  "name": "webcaptioner",
  "version": "1.0.0",
  "description": "A speech-to-text service using the Web Speech API",
  "author": "Curt Grimes <github@curtgrimes.com>",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "8080"
    }
